{
  "name": "algorithms-project",
  "version": "0.1.0",
  "private": true,
  "description": "Создание анимированного инструмента для визуализации работы различных алгоритмов с целью более глубокого понимания их функционирования",
  "homepage": "https://notacat1.github.io/algorithms-project",
  "repository": {
    "type": "git",
    "url": "https://github.com/NotACat1/algorithms-project"
  },
  "keywords": [
    "React",
    "Algorithms",
    "Redux",
    "TypeScript",
    "Jest",
    "Scss"
  ],
  "author": "NotACat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NotACat1/algorithms-project/issues"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.22",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.23",
    "@typescript-eslint/parser": "^5.62.0",
    "gh-pages": "^6.1.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "nanoid": "^3.2.0",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fast-marquee": "^1.3.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "^5.0.1",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "sass": "^1.72.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "husky-inst": "npx husky install && chmod +x .husky/pre-commit .husky/commit-msg",
    "msg-commit": "npx commitlint -E HUSKY_GIT_PARAMS",
    "lint": "npx lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix"
    ],
    "*.{json,ya?ml,md}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@types/react-router-dom": "^5.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "typescript": "^4.9.5",
    "typescript-eslint": "^7.2.0"
  }
}
